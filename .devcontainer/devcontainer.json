{
  "name": "MongoDB and Redis Dev Environment",
  "dockerComposeFile": "../docker/docker-compose.yml",
  "service": "mongodb",
  "workspaceFolder": "/workspace",
  "customizations": {
    "vscode": {
      "extensions": [
        "mongodb.mongodb-vscode",
        "ms-azuretools.vscode-docker"
      ]
    }
  },
  "ports": [27017, 6379],
  "postCreateCommand": "npm install"
}
